<%- include("header") -%>

<nav class="navbar navbar-expand-md navbar-dark fixed-top bg-danger">
  <a class="navbar-brand" href="#">Laundry-Hub</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
    aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

  <div class="collapse navbar-collapse" id="navbarSupportedContent">

    <ul class="navbar-nav ml-auto">
      <li class="nav-item">
        <a class="nav-link" href="/">Home</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/dashboard">dashboard</a>
      </li>

    </ul>

  </div>
</nav>


<div class = "container">
    <div class="jumbotron jumbotron-fluid" style="text-align: center">
        <div style="text-align: center">
          <h1 class="display-4">Transactions</h1>
        </div>
      </div>
      
         <div style="text-align: center">
              <table class="table">
                <thead>
                  <tr>
                    <th scope="col">created</th>
                    <th scope="col">dropoff deadline</th>
                    <th scope="col">fee</th>
                    <th scope="col">status</th>
                    <th scope="col">deliveryID</th>
                  </tr>
                </thead>
                <tbody>
              
                  
                  
                  
                        <tr>

                                <% for( let i = 0; i<DeliveriesCreated.length; i++) { %> 
                                  <td><%=DeliveriesCreated[i]  %> </td>
                                  <td><%=Deliveriesdropoffdeadline[i]  %> </td>
                                  <td><%=DeliveriesFees[i]  %> </td>
                                  <td><%=DeliveriesStatuses[i]  %> </td>
                                  <td><%=DeliveriesIds[i]  %> </td>
                                  <% } %>
  
                        </tr>
                   
                
              
                </tbody>
              </table>
            </div>

         
      <form action="/" class="form-signin">
        <button class="btn btn-danger btn-sm btn-block" type="submit">Cancel Transaction</button>
    </form>
      

</div>

<%- include("footer") -%>
